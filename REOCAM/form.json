{
    "elements": [
        {
            "type": "ValidationTextBox",
            "name": "CameraIP",
            "caption": "Kamera-IP"
        },
        {
            "type": "ValidationTextBox",
            "name": "Username",
            "caption": "Benutzername"
        },
        {
            "type": "PasswordTextBox",
            "name": "Password",
            "caption": "Passwort"
        },
        {
            "type": "Select",
            "name": "StreamType",
            "caption": "Stream-Typ",
            "options": [
                {
                    "caption": "Main Stream",
                    "value": "main"
                },
                {
                    "caption": "Sub Stream",
                    "value": "sub"
                }
            ]
        },
        {
            "type": "CheckBox",
            "name": "ShowWebhookVariables",
            "caption": "Webhook-Variablen anzeigen"
        },
        {
            "type": "CheckBox",
            "name": "ShowBooleanVariables",
            "caption": "Bewegungs-Variablen anzeigen"
        },
        {
            "type": "CheckBox",
            "name": "ShowSnapshots",
            "caption": "Schnappsch√ºsse anzeigen"
        },
        {
            "type": "CheckBox",
            "name": "ShowArchives",
            "caption": "Archivbilder anzeigen"
        },
        {
            "type": "NumberSpinner",
            "name": "MaxArchiveImages",
            "caption": "Maximale Archivbilder",
            "minimum": 1,
            "maximum": 100,
            "suffix": "Bilder"
        }
    ],
    "actions": [
        {
            "type": "Button",
            "caption": "Test-Schnappschuss",
            "onClick": "REOCAM_CreateTestSnapshot($id);"
        }
    ]
}
